{
  "name": "espression",
  "version": "1.1.1",
  "description": "Small and customizable EcmaScript expression parser and static eval, with support for most ES6 expression syntax and jsonPath.",
  "author": "Adrian Panella <ianchi74@outlook.com>",
  "license": "MIT",
  "repository": "https://github.com/ianchi/ESpression.git",
  "main": "dist/bundle/espression.cjs.js",
  "module": "dist/esm5/espression.js",
  "es2015": "dist/esm2015/espression.js",
  "types": "dist/src/main.d.ts",
  "private": false,
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "esprima": "^4.0.1",
    "jsep": "^0.3.4",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.7",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.0",
    "rollup-plugin-uglify": "^6.0.0",
    "standard-version": "^6.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.9",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "dev": "rollup -w",
    "test": "node test/test.js",
    "lint": "tslint -p . ",
    "prerelease": "npm run lint && npm run build && npm run test",
    "release": "standard-version && npm publish && git push --follow-tags"
  },
  "keywords": [
    "parser",
    "expression",
    "static-eval",
    "eval",
    "jsonpath",
    "ast",
    "reactive eval",
